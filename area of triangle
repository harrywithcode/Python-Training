import math
def point_distance(x1,y1,x2,y2):
    return math.sqrt((x1-x2)**2+(y1-y2)**2)

def area(x1,y1,x2,y2,x3,y3):
    a=point_distance(x1,y1,x2,y2)
    b=point_distance((x1+x2)/2,(y1+y2)/2,x3,y3)
    
    
    s=(a*b)/2
    
    return s
    
    
def test (x1,y1,x2,y2,x3,y3):
    print str(area(x1,y1,x2,y2,x3,y3))
   
    
test(0, 0, 0, 1, 1, 0)
