def is_intercept(a,b,c,d):
    return(a<=b and c<=d and c<=b and a<=d)
###a<=d is important###
def test(a,b,c,d):
    if is_intercept(a,b,c,d):
        print " these two intervals are intercept"
    else:
        print "these two intervals are not intercept"
        
test(1,2,3,4)
test(1,3,2,4)
test(2,3,1,4)
